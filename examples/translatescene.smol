class Scene(Int scaling)
    Int getScale()
        return this.scaling;
    end
end

class Rectangle(Scene scene, Int w, Int h, String name)
    Int area()
        Int s := this.scene.getScale();
        return s*this.w*this.h;
    end
end

main
    Scene sc := new Scene(2);
    Rectangle r := new Rectangle(sc, 5, 1, "rect1");
    Int res := r.area();
    print(res);
end
