query SELECT ?obj WHERE { ?obj smol:instanceOf prog:GeoUnit. ?obj :sealing :1}
query SELECT ?obj ?dir ?elem WHERE { ?obj smol:instanceOf :GeoUnit. ?obj ?dir ?elem. ?elem smol:instanceOf prog:Fault}
